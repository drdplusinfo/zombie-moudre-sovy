{
    "name": "drdplus/chceme-moudrou-sovu",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "Dračí hlídka",
        "DH",
        "Dračí doupě",
        "prosba",
        "zmrtvýchvstání"
    ],
    "autoload": {
        "psr-4": {
            "DrdPlus\\": "DrdPlus/"
        }
    },
    "require": {
        "php": ">=7.1",
        "drdplus/rules-skeleton": "^3.0"
    },
    "require-dev": {
        "composer/composer": "^1.8",
        "granam/exceptions-hierarchy": "^5.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^8.3",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "pre-autoload-dump": [
            "rm -f DrdPlus/Tests/RulesSkeleton && ln -s ../../vendor/drdplus/rules-skeleton/DrdPlus/Tests/RulesSkeleton DrdPlus/Tests/"
        ]
    }
}
